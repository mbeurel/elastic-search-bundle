{
  "name":                                     "austral/elastic-search-bundle",
  "type":                                     "symfony-bundle",
  "description":                              "Austral Elastic Search Bundle",
  "keywords":                                 ["Austral", "Elastic Search", "Symfony"],
  "license":                                  "MIT",
  "authors": [
    {
      "name":                                 "Matthieu Beurel",
      "email":                                "matthieu@austral.dev",
      "role":                                 "Developer"
    }
  ],
  "require": {
    "php":                                    "^8.0 || ^8.1 || ^8.2",

    "austral/tools-bundle":                   "^3.1",
    "austral/entity-bundle":                  "^3.1",

    "symfony/config":                         "^5.4 || ^6.4",
    "symfony/event-dispatcher":               "^5.4 || ^6.4",
    "elasticsearch/elasticsearch":            "^v7.17.0"
  },
  "require-dev": {
  },
  "suggest": {
    "austral/admin-bundle":                   "*@dev",
    "austral/website-bundle":                 "*@dev"
  },
  "config": {
    "sort-packages":                          true
  },
  "autoload": {
    "psr-4": { "Austral\\ElasticSearchBundle\\": "" }
  },
  "minimum-stability":                        "dev"
